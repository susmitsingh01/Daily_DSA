   "source": [
    "# insertion sort\n",
    "\n",
    "def insertion_sort(arr):\n",
    "    n = len(arr)\n",
    "\n",
    "    for i in range(1, n):\n",
    "        key = arr[i]\n",
    "        j = i-1\n",
    "        while j >= 0 and arr[j] > key:\n",
    "            arr[j+1] = arr[j]\n",
    "            j-=1\n",
    "\n",
    "        arr[j+1] = key\n",
    "\n",
    "arr = [5,4,3,2,1]\n",
    "insertion_sort(arr)\n",
    "arr\n"
   ]
  }
 ],
